<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GetUsers</servlet-name>
        <servlet-class>servlets.GetUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetFeeder</servlet-name>
        <servlet-class>servlets.SetFeeder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetFeeders</servlet-name>
        <servlet-class>servlets.GetFeeders</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSchedules</servlet-name>
        <servlet-class>servlets.GetSchedules</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetSchedule</servlet-name>
        <servlet-class>servlets.SetSchedule</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetUsers</servlet-name>
        <url-pattern>/GetUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetFeeder</servlet-name>
        <url-pattern>/SetFeeder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetFeeders</servlet-name>
        <url-pattern>/GetFeeders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSchedules</servlet-name>
        <url-pattern>/GetSchedules</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetSchedule</servlet-name>
        <url-pattern>/SetSchedule</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
